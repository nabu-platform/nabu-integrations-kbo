<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<crud>
    <allowHeaderAsQueryParameter>true</allowHeaderAsQueryParameter>
    <broadcastCreate>false</broadcastCreate>
    <broadcastUpdate>false</broadcastUpdate>
    <coreType>nabu.integrations.kbo.database.types.kboDownload</coreType>
    <createRole>$user</createRole>
    <deleteRole>$user</deleteRole>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>delta</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>contact</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>activity</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>address</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>processed</key>
        <operator>is null</operator>
        <or>false</or>
        <alias>unprocessed</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>delta</key>
        <operator>&lt;&gt;</operator>
        <or>false</or>
        <alias>full</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
